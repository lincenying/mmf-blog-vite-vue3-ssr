{
  "name": "mmf-blog-vite-vue3-ssr",
  "version": "2.0.1",
  "author": "lincenying <lincenying@qq.com>",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "del": "sh delete-ts.sh",
    "serve": "nodemon server",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "cross-env VITE_APP_SSR=true vite build --ssr src/entry-server.ts --outDir dist/server",
    "generate": "vite build --ssrManifest --outDir dist/static && npm run build:server && node prerender",
    "start": "cross-env NODE_ENV=production node server",
    "debug": "node --inspect-brk server",
    "workbox": "workbox generateSW workbox-config.js",
    "lint": "eslint './src/**/*.{js,ts,tsx,vue}'",
    "lint:fix": "eslint './src/**/*.{js,ts,tsx,vue}' --fix"
  },
  "dependencies": {
    "@kangc/v-md-editor": "^2.3.15",
    "@vueuse/core": "^9.13.0",
    "@vueuse/head": "^1.1.23",
    "axios": "^1.3.4",
    "element-plus": "^2.3.1",
    "express": "^4.18.2",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "pinia": "^2.0.33",
    "qs": "^6.11.1",
    "store2": "^2.14.2",
    "vue": "3.2.47",
    "vue-content-loader": "^2.0.1",
    "vue-loading-overlay": "^6.0.3",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/js-cookie": "^3.0.3",
    "@types/lodash": "^4.14.191",
    "@types/md5": "^2.3.2",
    "@types/morgan": "^1.9.4",
    "@types/serve-static": "^1.15.1",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/compiler-sfc": "^3.2.47",
    "@vue/server-renderer": "^3.2.47",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "eslint-config-lcy-vue3": "^1.0.5",
    "eslint-plugin-prettier": "^4.2.1",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "lru-cache": "^8.0.4",
