{
  "name": "mmf-blog-vite-vue3-ssr",
  "type": "module",
  "version": "1.25.1029",
  "private": true,
  "packageManager": "pnpm@10.27.0",
  "author": "lincenying <lincenying@qq.com>",
  "engines": {
    "node": ">=16.13.0"
  },
  "scripts": {
    "serve": "nodemon --watch ./server.dev.ts --exec tsx ./server.dev.ts",
    "build": "tsx polyfill.ts && npm run build:client && npm run build:server && npm run build:server:entry",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "cross-env VITE_APP_SSR=true vite build --ssr src/entry-server.ts --outDir dist/server",
    "build:server:entry": "tsup --entry.server server.prod.ts --format esm --target es2020",
    "start": "cross-env NODE_ENV=production node ./dist/server.js",
    "generate": "vite build --ssrManifest --outDir dist/static && npm run build:server && tsx ./prerender.ts",
    "workbox": "workbox generateSW workbox-config.js",
    "lint": "eslint .",
    "lint:ts": "vue-tsc -b --noEmit",
    "lint:fix": "eslint . --fix",
    "lint:css": "stylelint '**/*.{css,scss}' --fix --allow-empty-input",
    "release": "bumpp",
    "prepare[-del]": "npx simple-git-hooks",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@kangc/v-md-editor": "^2.3.18",
    "@lincy/utils": "^0.7.3",
    "@unhead/ssr": "^2.1.1",
    "@unhead/vue": "^2.1.1",
    "@vue/server-renderer": "^3.5.26",
    "@vueuse/core": "^14.1.0",
    "axios": "^1.13.2",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "element-plus": "^2.13.0",
    "express": "^5.2.1",
    "http-proxy-middleware": "^3.0.5",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "lru-cache": "^11.2.4",
    "md5": "^2.3.0",
    "morgan": "^1.10.1",
    "pinia": "^3.0.4",
    "prismjs": "^1.30.0",
    "qs": "^6.14.1",
    "request-ip": "^3.3.0",
    "serve-static": "^2.2.1",
    "store2": "^2.14.4",
    "vue": "^3.5.26",
    "vue-content-loader": "^2.0.1",
    "vue-loading-overlay": "^6.0.6",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.2.3",
    "@lincy/eslint-config": "^6.3.3",
    "@lincy/stylelint-config": "^2.2.0",
    "@lincy/unocss-base-config": "^2.4.1",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.6",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.17.21",
    "@types/md5": "^2.3.6",
    "@types/morgan": "^1.9.10",
    "@types/polyfill-library": "^3.108.4",
    "@types/prismjs": "^1.26.5",
    "@types/request-ip": "^0.0.41",
    "@types/serve-static": "^2.2.0",
    "@unocss/eslint-plugin": "^66.5.12",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitejs/plugin-vue-jsx": "^5.1.3",
    "@vue/compiler-sfc": "^3.5.26",
    "bumpp": "^10.3.2",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-plugin-format": "^1.1.0",
    "express-rate-limit": "^8.2.1",
    "lint-staged": "^16.2.7",
    "mockjs": "^1.1.0",
    "nodemon": "^3.1.11",
    "polyfill-library": "^4.8.0",
    "postcss": "^8.5.6",
    "sass": "^1.97.1",
    "simple-git-hooks": "^2.13.1",
    "stylelint": "^16.26.1",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "unocss": "^66.5.12",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-vue-components": "^30.0.0",
    "unplugin-vue-macros": "^2.14.5",
    "vite": "^7.3.0",
    "vite-plugin-mock": "^3.0.2",
    "vite-plugin-pwa": "^1.2.0",
    "vue-tsc": "^3.2.1"
  },
  "simple-git-hooks[-del]": {
    "pre-commit": "npm run lint-staged"
  },
  "lint-staged[-del]": {
    "*.{[jt]s?(x),vue,md}": "eslint --fix",
    "*.{css,scss}": "stylelint --fix --allow-empty-input"
  }
}
