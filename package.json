{
  "name": "mmf-blog-vite-vue3-ssr",
  "version": "1.0.0",
  "author": "lincenying <lincenying@qq.com>",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "serve": "node server",
    "build": "yarn build:client && yarn build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "cross-env VITE_APP_SSR=true vite build --ssr src/entry-server.js --outDir dist/server",
    "generate": "vite build --ssrManifest --outDir dist/static && yarn build:server && node prerender",
    "start": "cross-env NODE_ENV=production node server",
    "debug": "node --inspect-brk server",
    "workbox": "workbox generateSW workbox-config.js",
    "lint": "eslint './src/**/*.{js,ts,tsx,vue}'",
    "lint:fix": "eslint './src/**/*.{js,ts,tsx,vue}' --fix"
  },
  "dependencies": {
    "@kangc/v-md-editor": "^2.2.3",
    "@vueuse/core": "^5.0.3",
    "@vueuse/head": "^0.6.0",
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "qs": "^6.10.1",
    "store2": "^2.12.0",
    "toastr": "^2.1.4",
    "vue": "^3.0.5",
    "vue-content-loader": "^2.0.0",
    "vue-router": "^4.0.8",
    "vuex": "^4.0.1",
    "vuex-router-sync": "^5.0.0",
    "windicss": "^3.0.12"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@vitejs/plugin-legacy": "^1.4.1",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.1.1",
    "@vue/server-renderer": "^3.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.3",
    "eslint-config-lcy-vue": "^3.1.2",
    "http-proxy-middleware": "^2.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "lru-cache": "^6.0.0",
    "morgan": "^1.10.0",
    "postcss": "^8.3.4",
    "prettier": "^2.3.1",
    "sass": "^1.35.1",
    "vite": "^2.3.7",
    "vite-plugin-pwa": "^0.8.1",
    "vite-plugin-style-import": "^1.0.0",
    "vite-plugin-windicss": "^1.0.4",
    "workbox-cli": "^6.1.5"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue}": [
      "eslint --fix"
    ]
  }
}
