    "prettier": "^2.8.7",
    "sass": "^1.60.0",
    "stylelint": "^15.3.0",
    "stylelint-config-lcy": "^1.15.10",
    "typescript": "^5.0.2",
    "unocss": "^0.50.6",
    "unplugin-auto-import": "^0.15.2",
    "unplugin-vue-components": "^0.24.1",
    "unplugin-vue-macros": "^1.11.1",
    "vite": "4.2.1",
