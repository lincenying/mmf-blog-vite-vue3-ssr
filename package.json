{
  "name": "mmf-blog-vite-vue3-ssr",
  "type": "module",
  "version": "2.0.1",
  "private": true,
  "packageManager": "pnpm@8.4.0",
  "author": "lincenying <lincenying@qq.com>",
  "engines": {
    "node": ">=16.13.0"
  },
  "scripts": {
    "prepare": "husky install",
    "del": "sh delete-ts.sh",
    "serve": "node server",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "cross-env VITE_APP_SSR=true vite build --ssr src/entry-server.ts --outDir dist/server",
    "generate": "vite build --ssrManifest --outDir dist/static && npm run build:server && node prerender",
    "start": "cross-env NODE_ENV=production node server",
    "debug": "node --inspect-brk server",
    "workbox": "workbox generateSW workbox-config.js",
    "ts": "vue-tsc --noEmit",
    "lint": "eslint ./src --ext .vue,.ts,.tsx",
    "lint:fix": "eslint ./src --ext .vue,.ts,.tsx --fix"
  },
  "dependencies": {
    "@kangc/v-md-editor": "^2.3.15",
    "@lincy/utils": "^0.3.0",
    "@vueuse/core": "^10.2.0",
    "@vueuse/head": "^1.1.26",
    "axios": "^1.4.0",
    "element-plus": "^2.3.6",
    "express": "^4.18.2",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "pinia": "^2.1.4",
    "prismjs": "^1.29.0",
    "qs": "^6.11.2",
    "store2": "^2.14.2",
    "vue": "3.3.4",
    "vue-content-loader": "^2.0.1",
    "vue-loading-overlay": "^6.0.3",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@lincy/eslint-config": "^2.0.5",
    "@lincy/stylelint-config": "^1.15.10",
    "@lincy/vite-plugin-mock": "^3.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/js-cookie": "^3.0.3",
    "@types/lodash": "^4.14.195",
    "@types/md5": "^2.3.2",
    "@types/morgan": "^1.9.4",
    "@types/prismjs": "^1.26.0",
    "@types/serve-static": "^1.15.1",
    "@unocss/eslint-config": "^0.53.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/server-renderer": "^3.3.4",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.43.0",
    "eslint-plugin-prettier": "^4.2.1",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "lru-cache": "^10.0.0",
    "mockjs": "^1.1.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.22",
    "polyfill-library": "^4.7.0",
    "postcss": "^8.4.24",
    "sass": "^1.63.5",
    "stylelint": "^15.8.0",
    "typescript": "^5.1.3",
    "unocss": "^0.53.1",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-vue-components": "^0.25.1",
    "unplugin-vue-macros": "^2.3.0",
    "vite": "4.3.9",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mock": "^3.0.0",
    "vite-plugin-pwa": "^0.16.4",
    "vue-tsc": "^1.8.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup",
        "esbuild",
        "postcss",
        "prettier"
      ]
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue}": [
      "eslint --fix"
    ]
  }
}
