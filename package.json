{
  "name": "mmf-blog-vite-vue3-ssr",
  "type": "module",
  "version": "2.0.2",
  "private": true,
  "packageManager": "pnpm@8.6.1",
  "author": "lincenying <lincenying@qq.com>",
  "engines": {
    "node": ">=16.13.0"
  },
  "scripts": {
    "prepare": "sh simple-git-hooks.sh",
    "serve": "nodemon --watch ./server.dev.ts --exec tsx ./server.dev.ts",
    "build": "tsx polyfill.ts && npm run build:client && npm run build:server && npm run build:server:entry",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "cross-env VITE_APP_SSR=true vite build --ssr src/entry-server.ts --outDir dist/server",
    "build:server:entry": "tsup --entry.server server.prod.ts --format esm --target es2020",
    "start": "cross-env NODE_ENV=production node ./dist/server.js",
    "generate": "vite build --ssrManifest --outDir dist/static && npm run build:server && tsx ./prerender.ts",
    "workbox": "workbox generateSW workbox-config.js",
    "lint": "eslint .",
    "lint:ts": "vue-tsc --noEmit",
    "lint:fix": "eslint . --fix",
    "lint:css": "stylelint '**/*.{css,scss}' --fix --allow-empty-input"
  },
  "dependencies": {
    "@kangc/v-md-editor": "^2.3.18",
    "@lincy/utils": "^0.5.3",
    "@unhead/ssr": "^1.8.10",
    "@unhead/vue": "^1.8.10",
    "@vueuse/core": "^10.8.0",
    "axios": "^1.6.7",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "element-plus": "^2.5.6",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "lru-cache": "^10.2.0",
    "md5": "^2.3.0",
    "morgan": "^1.10.0",
    "pinia": "^2.1.7",
    "prismjs": "^1.29.0",
    "qs": "^6.11.2",
    "request-ip": "^3.3.0",
    "serve-static": "^1.15.0",
    "store2": "^2.14.3",
    "vue": "3.4.19",
    "vue-content-loader": "^2.0.1",
    "vue-loading-overlay": "^6.0.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@lincy/base-config": "^1.1.0",
    "@lincy/eslint-config": "^4.2.5",
    "@lincy/stylelint-config": "^1.17.4",
    "@lincy/vite-plugin-mock": "^3.0.0",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.14.202",
    "@types/md5": "^2.3.5",
    "@types/morgan": "^1.9.9",
    "@types/polyfill-library": "^3.108.4",
    "@types/prismjs": "^1.26.3",
    "@types/request-ip": "^0.0.41",
    "@types/serve-static": "^1.15.5",
    "@unocss/eslint-plugin": "^0.58.5",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/compiler-sfc": "^3.4.19",
    "@vue/server-renderer": "^3.4.19",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-plugin-format": "^0.1.0",
    "lint-staged": "^15.2.2",
    "mockjs": "^1.1.0",
    "nodemon": "^3.0.3",
    "polyfill-library": "^4.8.0",
    "postcss": "^8.4.35",
    "sass": "^1.71.1",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^16.2.1",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "unocss": "^0.58.5",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-macros": "^2.7.10",
    "vite": "5.1.3",
    "vite-plugin-mock": "^3.0.1",
    "vite-plugin-pwa": "^0.19.0",
    "vue-tsc": "^1.8.27"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup",
        "esbuild",
        "postcss",
        "prettier"
      ]
    }
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{[jt]s?(x),vue,md}": "eslint --fix",
    "*.{css,scss}": "stylelint --fix --allow-empty-input"
  }
}
