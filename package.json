{
  "name": "mmf-blog-vite-vue3-ssr",
  "version": "1.0.0",
  "author": "lincenying <lincenying@qq.com>",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "serve": "node server",
    "build": "yarn build:client && yarn build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "cross-env VITE_APP_SSR=true vite build --ssr src/entry-server.js --outDir dist/server",
    "generate": "vite build --ssrManifest --outDir dist/static && yarn build:server && node prerender",
    "start": "cross-env NODE_ENV=production node server",
    "debug": "node --inspect-brk server",
    "workbox": "workbox generateSW workbox-config.js",
    "lint": "eslint './src/**/*.{js,ts,tsx,vue}'",
    "lint:fix": "eslint './src/**/*.{js,ts,tsx,vue}' --fix"
  },
  "dependencies": {
    "@kangc/v-md-editor": "next",
    "@vueuse/core": "^9.13.0",
    "@vueuse/head": "^1.1.19",
    "axios": "^1.3.4",
    "express": "^4.18.2",
    "js-cookie": "^3.0.0",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "pinia": "^2.0.33",
    "qs": "^6.11.1",
    "store2": "^2.14.2",
    "toastr": "^2.1.4",
    "vue": "3.2.47",
    "vue-content-loader": "^2.0.1",
    "vue-router": "^4.1.6",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/compiler-sfc": "^3.2.47",
    "@vue/server-renderer": "^3.2.47",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "eslint-config-lcy-vue": "^3.4.0",
    "eslint-plugin-prettier": "^4.2.1",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "lru-cache": "^7.18.3",
    "morgan": "^1.10.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "sass": "^1.58.3",
    "stylelint": "^15.2.0",
    "stylelint-config-lcy": "^1.15.10",
    "typescript": "^4.9.5",
    "unocss": "^0.50.4",
    "unplugin-auto-import": "^0.15.1",
    "unplugin-vue-components": "^0.24.1",
    "unplugin-vue-macros": "^1.9.1",
    "vite": "4.1.4",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-pwa": "^0.14.4"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue}": [
      "eslint --fix"
    ]
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "packageManager": "pnpm@7.29.1"
}
