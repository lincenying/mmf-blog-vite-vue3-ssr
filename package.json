{
  "name": "mmf-blog-vite-vue3-ssr",
  "version": "1.0.0",
  "author": "lincenying <lincenying@qq.com>",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "serve": "node server",
    "build": "yarn build:client && yarn build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "cross-env VITE_APP_SSR=true vite build --ssr src/entry-server.js --outDir dist/server",
    "generate": "vite build --ssrManifest --outDir dist/static && yarn build:server && node prerender",
    "start": "cross-env NODE_ENV=production node server",
    "debug": "node --inspect-brk server",
    "workbox": "workbox generateSW workbox-config.js",
    "lint": "eslint './src/**/*.{js,ts,tsx,vue}'",
    "lint:fix": "eslint './src/**/*.{js,ts,tsx,vue}' --fix"
  },
  "dependencies": {
    "@kangc/v-md-editor": "^2.2.3",
    "@vueuse/core": "^7.4.1",
    "@vueuse/head": "^0.7.4",
    "axios": "^0.24.0",
    "express": "^4.17.2",
    "js-cookie": "^3.0.0",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "qs": "^6.10.2",
    "store2": "^2.13.1",
    "toastr": "^2.1.4",
    "vue": "3.2.26",
    "vue-content-loader": "^2.0.1",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2",
    "vuex-router-sync": "^5.0.0",
    "windicss": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.16.5",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vue/compiler-sfc": "^3.2.26",
    "@vue/server-renderer": "^3.2.26",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "eslint-config-lcy-vue": "^3.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "http-proxy-middleware": "^2.0.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.3",
    "lru-cache": "^6.0.0",
    "morgan": "^1.10.0",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "sass": "^1.45.1",
    "vite": "2.7.6",
    "vite-plugin-pwa": "^0.11.12",
    "vite-plugin-style-import": "^1.4.0",
    "vite-plugin-windicss": "^1.6.1",
    "workbox-cli": "^6.4.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue}": [
      "eslint --fix"
    ]
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  }
}
